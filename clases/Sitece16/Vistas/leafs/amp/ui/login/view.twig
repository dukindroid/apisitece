{% extends '/amp/base.amp.twig' %}
{% block title %}Bienvenido!⚡{% endblock %}
{% block content %}
    
{% raw %}
<form method="post" action-xhr="https://apisitece.ddns.net/subscribe" target="_blank">
  <fieldset>
    <label>
      <span># Usuario</span>
      <input type="text" name="name" required>
    </label>
    <label>
      <span>Contraseña</span>
      <input type="email" name="email" required>
    </label>
    <input type="submit" value="Subscribe">
  </fieldset>
  <div submit-success>
    <template type="amp-mustache">
      Subscription successful!
    </template>
  </div>
  <div submit-error>
    <template type="amp-mustache">
      Subscription failed!
    </template>
  </div>
</form>
{% endraw %}

{% endblock %}